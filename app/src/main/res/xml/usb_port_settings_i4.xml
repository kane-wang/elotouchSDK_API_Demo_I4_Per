<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
        android:title="@string/usb_port_control_title">
        <SwitchPreference
            android:key="key_usb_port1"
            android:title="USB port 1 (TYPE-A)"
            android:summaryOn="TYPE-A USB port on board"
            android:summaryOff="TYPE-A USB port on board"
            android:persistent="false" />

        <SwitchPreference
            android:key="key_usb_port2"
            android:title="USB port 2 (TYPE-C)"
            android:summaryOn="TYPE-C USB port on board"
            android:summaryOff="TYPE-C USB port on board"
            android:persistent="false" />

        <SwitchPreference
            android:key="key_usb_port3"
            android:title="USB port 3 (Micro USB)"
            android:summaryOn="Micro-USB port at left side"
            android:summaryOff="Micro-USB port at left side"
            android:persistent="false" />
    
       <SwitchPreference
            android:key="key_usb_port4"
            android:title="USB port 4 (Micro USB)"
            android:summaryOn="Micro-USB port at top side"
            android:summaryOff="Micro-USB port at top side"
            android:persistent="false" />
        
        <SwitchPreference
            android:key="key_usb_port5"
            android:title="USB port 5 (Micro USB)"
            android:summaryOn="Micro-USB port at right side (near power port)"
            android:summaryOff="Micro-USB port at right side (near power port)"
            android:persistent="false" />

        <SwitchPreference
            android:key="key_usb_port6"
            android:title="USB port 6 (Micro USB)"
            android:summaryOn="Micro-USB port at bottom side"
            android:summaryOff="Micro-USB port at bottom side"
            android:persistent="false" />
</PreferenceScreen>
